CREATE VIEW `get_myfriends` AS select `cc`.`connect_from` AS `connect_from`,`cc`.`connect_to` AS `connect_to`,`u`.`name` AS `name`,`u`.`email` AS `email`,`v1`.`value` AS `value`,`cu`.`thumb` AS `thumb` from (((`#__community_connection` `cc` left join `#__users` `u` on((`u`.`id` = `cc`.`connect_to`))) left join `#__community_fields_values` `v1` on(((`u`.`id` = `v1`.`user_id`) and (`v1`.`field_id` = 18)))) left join `#__community_users` `cu` on((`u`.`id` = `cu`.`userid`))) where (`cc`.`status` = 1);
